<configure xmlns="http://namespaces.zope.org/zope"
           xmlns:browser="http://namespaces.zope.org/browser">

  <include file="renderer.zcml" />

  <browser:pages
     for="..interfaces.IAmWicked"
     class='.wickedservice.WickedAdd'
     permission='zope.Public'>

    <browser:page
       name='wickedadd'
       attribute='addContent'
       />

    <browser:page
       name='wickedaddmenu'
       attribute='addMenu'
       />
  </browser:pages>

</configure>
